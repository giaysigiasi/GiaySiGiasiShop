@model IList<ProductSpecificationModel>
@using Nop.Web.Models.Catalog;
@if (Model.Count > 0)
{    
       for (int i = 0; i < Model.Count; i++)
        {
            var item = Model[i];
            <div class="normal-detail">
                <span>@item.SpecificationAttributeName:</span> @Html.Raw(item.ValueRaw)
            </div>
        }

}