@model IList<ManufacturerBriefInfoModel>
@using Nop.Web.Models.Catalog;
@if (Model.Count > 0)
{
    <div class="manufacturers normal-detail">
        @if (Model.Count == 1)
        {
            <span> @T("Products.Manufacturer"):</span>
        }
        else
        {
            <span> @T("Products.Manufacturers"):</span>
        }      
            @for (int i = 0; i < Model.Count; i++)
            {
                var item = Model[i];
                @item.Name
                @*<a href="@Url.RouteUrl("Manufacturer", new { SeName = item.SeName })">@item.Name</a>*@
                if (i != Model.Count - 1)
                {
                    <span class="separator">,</span>
                }
            }
        
    </div>
}